{
  "name": "pronostigol-v2",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "node --optimize_for_size --max-old-space-size=4096 node_modules/nuxt/bin/nuxt.js build",
    "start": "node --optimize_for_size --max-old-space-size=4096 node_modules/nuxt/bin/nuxt.js start",
    "generate": "nuxt generate",
    "apidoc": "node node_modules/apidoc/bin/apidoc -i api/ -o static/docs/ -c apidoc.config.js",
    "postinstall": "npm run apidoc",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/content": "1.14.0",
    "@nuxtjs/axios": "5.13.6",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/google-adsense": "1.4.0",
    "@nuxtjs/i18n": "7.0.3",
    "@nuxtjs/moment": "1.6.1",
    "@nuxtjs/sitemap": "2.4.0",
    "@nuxtjs/style-resources": "1.2.1",
    "apidoc": "0.50.3",
    "axios": "0.21.4",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "core-js": "3.17.3",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-validation": "3.0.8",
    "forcedomain": "2.2.11",
    "joi": "17.4.2",
    "mongoose": "6.0.5",
    "nuxt": "2.15.8",
    "nuxt-cookie-control": "1.9.8",
    "octonode": "0.10.2",
    "path": "0.12.7",
    "v-runtime-template": "1.10.0",
    "vue": "2.6.14",
    "vue-adblock-detector": "1.2.4",
    "vue-tweet-embed": "2.4.0",
    "vuetify": "2.5.8",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@nuxtjs/eslint-config": "6.0.1",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/vuetify": "1.12.1",
    "@vue/test-utils": "1.2.2",
    "babel-core": "6.26.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.2.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-import-resolver-webpack": "0.13.1",
    "eslint-plugin-nuxt": "2.0.0",
    "husky": "7.0.2",
    "jest": "27.2.0",
    "lint-staged": "11.1.2",
    "vue-jest": "3.0.7"
  },
  "engines": {
    "node": "12.18.1",
    "npm": "6.14.5"
  }
}
